{
  "name": "Videotek",
  "private": true,
  "version": "1.0.0",
  "description": "Plateforme de catalogue de films avec SSR",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "tsx server/index.ts",
    "dev:frontend": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr server/index.ts",
    "start": "NODE_ENV=production node dist/server/index.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "scan:movies": "tsx server/scripts/movieScanner.ts",
    "index:movies": "tsx server/scripts/autoIndexer.ts",
    "watch:movies": "tsx server/scripts/movieWatcher.ts",
    "admin:create": "tsx server/scripts/createAdmin.ts",
    "admin:migrate": "tsx server/scripts/migrateToRoles.ts",
    "test:smtp": "node test-smtp.mjs"
  },
